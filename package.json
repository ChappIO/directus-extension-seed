{
	"name": "directus-extension-seed",
	"description": "Seed your Directus instance using upserts from .yaml files",
	"icon": "extension",
	"version": "1.0.2",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-hook"
	],
	"files": [
		"dist",
		"README.md"
	],
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^9.22.4"
	},
	"scripts": {
		"prepack": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "9.22.4",
		"@types/glob": "^8.0.1",
		"@types/js-yaml": "^4.0.5",
		"@types/node": "^18.13.0",
		"js-yaml": "^4.1.0",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"glob": "^8.1.0"
	}
}
